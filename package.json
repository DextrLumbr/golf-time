{
  "name": "golf-score-app",
  "version": "1.0.0",
  "description": "Golf Score App",
  "main": "server.js",
  "engines": {
    "node": "15.x"
  },
  "scripts": {
    "devStart": "concurrently \"npm run server\" \"cd client && npm run start\"",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Kyle Chung",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.6",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^6.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "mongodb": "^3.6.6"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "type": "module"
}
